import csv  
import json  
  
# Open the CSV  
f = open( 'listings.csv', 'r' )  
# Change each fieldname to the appropriate field name. I know, so difficult.  
reader = csv.DictReader(f)  
# Parse the CSV into JSON  
out = json.dumps([row for row in reader])  
print ("JSON parsed!")  
# Save the JSON  
f = open( 'fuck.json', 'w')  
f.write(out)  
print ("JSON saved!")  

